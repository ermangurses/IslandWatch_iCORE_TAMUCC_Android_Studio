<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android1="http://schemas.android.com/apk/res/android"
    xmlns:android2="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayoutMain"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageView
        android:id="@+id/imageViewBackgroundMain"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:adjustViewBounds="true"
        android:baselineAlignBottom="true"
        android:contentDescription="TODO"
        android:cropToPadding="false"
        android:scaleType="fitXY"
        android:src="@drawable/tamucc_main_background" />

    <ImageButton
        android:id="@+id/imageButtonMainCallUdp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:layout_marginTop="155dp"
        android:adjustViewBounds="true"
        android:contentDescription="TODO"
        android:focusable="false"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/icore_call_udp"
        android2:paddingBottom="5dp" />

    <ImageButton
        android:id="@+id/imageButtonMainReportUdp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="false"
        android:layout_below="@+id/imageButtonMainCallUdp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:layout_marginTop="160dp"
        android:adjustViewBounds="true"
        android:contentDescription="TODO"
        android:paddingBottom="5dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:scaleType="fitXY"
        android:src="@drawable/icore_report_udp" />

    <TextView
        android:id="@+id/textViewMainCallUdp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/imageButtonMainCallUdp"
        android:layout_centerHorizontal="true"
        android2:layout_alignBottom="@id/imageButtonMainCallUdp"
        android:text="Call UPD"
        android:textColor="#000"
        android:textSize="30sp"
        android:textStyle="bold"
        android:typeface="serif"
        android2:gravity="center_vertical" />

    <TextView
        android:id="@+id/textViewMainReportUdp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/imageButtonMainReportUdp"
        android2:layout_alignBottom="@id/imageButtonMainReportUdp"
        android2:layout_centerHorizontal="true"
        android:text="Report"
        android:textColor="#000"
        android:textSize="30sp"
        android:textStyle="bold"
        android:typeface="serif"
        android2:gravity="center_vertical" />

    <ImageButton
        android:id="@+id/imageButtonMainBottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:adjustViewBounds="true"
        android:background="@drawable/bottom_button"
        android:baselineAlignBottom="false"
        android:contentDescription="TODO"
        android:cropToPadding="false"
        android:longClickable="true"
        android:scaleType="fitXY"
        android:src="@drawable/icore_co_brand_background" />

    <ImageView
        android:id="@+id/imageViewMainIcoreCoBrand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/imageButtonMainBottom"
        android:layout_centerHorizontal="true"
        android2:layout_alignBottom="@id/imageButtonMainBottom"
        android:adjustViewBounds="true"
        android:paddingBottom="3dp"
        android:paddingTop="3dp"
        android:scaleType="fitCenter"
        android:src="@drawable/icore_co_brand" />

</RelativeLayout>